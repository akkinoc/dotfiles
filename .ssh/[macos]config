Host github.com
    CheckHostIP no
    IdentityFile ~/.ssh/github
    StrictHostKeyChecking yes
    User git
    UserKnownHostsFile ~/.ssh/github.hosts

Host cyberz.github.com
    CheckHostIP no
    HostName github.com
    IdentityFile ~/.ssh/cyberz.github
    StrictHostKeyChecking yes
    User git
    UserKnownHostsFile ~/.ssh/cyberz.github.hosts

Host *
    AddKeysToAgent yes
    IdentitiesOnly yes
    ServerAliveCountMax 5
    ServerAliveInterval 60
